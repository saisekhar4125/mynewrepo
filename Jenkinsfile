pipeline {
  agent any
  stages {
    stage('build ') {
      steps {
        sh 'echo helloworld'
        echo 'hello world'
      }
    }

    stage('compile') {
      steps {
        echo 'this is compile'
      }
    }

    stage('deploy') {
      steps {
        echo 'this is deploy'
      }
    }

  }
}